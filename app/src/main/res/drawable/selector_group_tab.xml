<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <solid android:color="#c8851a"/>
            <corners android:radius="20dp"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <solid android:color="#1a2e50"/>
            <corners android:radius="20dp"/>
            <stroke android:width="1dp" android:color="#c8851a"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#e09820"/>
            <corners android:radius="20dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#162a50"/>
            <corners android:radius="20dp"/>
        </shape>
    </item>
</selector>
